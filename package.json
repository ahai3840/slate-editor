{
  "name": "slate-editor",
  "version": "0.6.8-rc.2",
  "description": "Another rich text editor using Slate framework with toolbar support",
  "homepage": "",
  "author": {
    "name": "Canner",
    "email": "chilijung@gmail.com",
    "url": ""
  },
  "files": [
    "lib",
    "dist"
  ],
  "main": "lib/index.js",
  "keywords": [
    "editor",
    "slate",
    "slate-editor"
  ],
  "devDependencies": {
    "antd": "^3.3.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.11.6",
    "chai": "^3.5.0",
    "cross-env": "^2.0.1",
    "css-loader": "^0.28.10",
    "enzyme": "^2.4.1",
    "eslint": "^4.18.2",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.66.0",
    "flow-copy-source": "^1.3.0",
    "immutable": "^4.0.0-rc.9",
    "markup-it": "^6.0.4",
    "prettier": "^1.9.2",
    "raw-loader": "^0.5.1",
    "react": "^16.2.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.2.0",
    "slate": "^0.33.0",
    "slate-react": "^0.12.0",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.16.0",
    "webpack-bundle-size-analyzer": "^2.7.0",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.1.0"
  },
  "scripts": {
    "lint": "prettier --write ./src/**/*.js && eslint ./src/**/*.js",
    "build:flow": "flow-copy-source -v -i '**/test/**' src lib",
    "clean": "rimraf lib dist",
    "build": "babel src --out-dir lib && npm run build:flow",
    "prepublish": "npm run clean && npm run check:src && npm run build",
    "check:src": "npm run lint",
    "start": "./node_modules/.bin/webpack-dev-server --config webpack.config.dev.js --mode development",
    "build:docs": "./node_modules/.bin/webpack --config webpack.config.prod.js --mode production",
    "build:dist": "./node_modules/.bin/webpack --config webpack.config.umd.js --mode production",
    "validate": "npm ls",
    "build:slate-constant": "babel ./plugins/slate-constant/src -d .plugin/slate-constant/lib --copy-files"
  },
  "pre-commit": [
    "lint"
  ],
  "repository": "slate-editor",
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0 || 16.x",
    "react-dom": "^0.14.0 || ^15.0.0 || 16.x",
    "slate": "^0.33.0",
    "slate-react": "^0.12.0"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "classnames": "2.2.6",
    "create-table-picker": "^0.1.2",
    "cross-spawn": "6.0.5",
    "emoji-mart": "2.7.0",
    "github-markdown-css": "^2.10.0",
    "is-hotkey": "0.1.3",
    "is-windows": "1.0.2",
    "js-video-url-parser": "0.2.7",
    "lodash.mapvalues": "4.6.0",
    "lodash.omit": "^4.5.0",
    "object-values": "^2.0.0",
    "quill-icons": "0.6.0",
    "react-icons": "2.2.7",
    "react-intl": "^2.4.0",
    "react-loading-image": "0.4.1",
    "react-resizable": "1.7.5",
    "slate-edit-list": "^0.11.2",
    "slate-edit-table": "0.15.2",
    "slate-trailing-block": "^0.5.0",
    "styled-components": "^3.2.3",
    "get-selection-range": "^0.1.0",
    "react-window-detect-dimensions": "^0.2.0"
  }
}
